<% layout('layouts/boilerplate') -%>
<h1>Workspots index</h1>

<% workspots.forEach(function(workspot) { %>
<div>
	<h2><%= workspot.name %></h2>
	<div><%= workspot.type %></div>
	<div><%= workspot.description %></div>
	<div>wifi: <%= workspot.wifi %></div>
	<div><%= workspot.location %></div>
	<div><a href="/workspots/<%= workspot.id %>">View More</a></div>
</div>
<% }); %>